@model List<Itn.Shared.Transaction.DiPurchaseOrderListItem>
<div>
    <table class="table table-striped">
        <thead>
            <tr>
                <th scope="col">PONbr</th>
                <th scope="col">PO Status</th>
                <th scope="col">Customer Nbr</th>
                <th scope="col">Customer Name</th>
                <th scope="col">Delivery Date</th>
            </tr>
        </thead>
        <tbody>
        @if (Model.Count == 0)
        {
            <tr> <td colspan="5"> <span class="error text-danger">No results found</span></td></tr>
        }
        else
        {
            foreach (var item in @Model)
             {
                 <tr>
                
                     <td>@item.PoNbr</td>
                     <td>@item.Status</td>
                     <td>@item.CustomerNbr</td>
                     <td>@item.CustomerName</td>
                     <td>@item.DeliveryDate</td>
                 </tr>
             }

        }

      
        </tbody>
    </table>
</div>